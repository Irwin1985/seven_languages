object aaa {
  println("Welcome to the Scala worksheet")
  println("TDSFFDS")
  var aaa = List("ee","fdfdsf","gfdr")
  for ( x <- aaa) {  println(x) }
  
  for (i <- List(1,2,3)) yield i * 2
  
  var myList = List(1,2,4,65,32,2,3)
  var sum = myList.foldLeft(0)((sum, value) => sum + value)
  var max = myList.foldLeft(0)((max, value) => {
  	//if (value > max) yield value
  	yield max
  })
  
}