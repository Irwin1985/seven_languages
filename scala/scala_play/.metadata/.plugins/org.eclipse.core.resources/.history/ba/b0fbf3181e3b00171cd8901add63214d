object Day_2_total_size_of_strings {
  println("Welcome to the Scala worksheet")
  
  //Use foldLeft to caculate total size of a list of strings
  
   var myList = List("DDE", "fdsd", "ddff", "dfdsdfs")
   var res = myList.foldLeft(0)(
   	(running, item) => {
   		running + item.length()
   	)
   }
   
   
}