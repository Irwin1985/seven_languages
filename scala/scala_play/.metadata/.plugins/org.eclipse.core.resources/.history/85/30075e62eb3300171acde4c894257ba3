

object MyFirstApp extends App {
  override def main(args: Array[String]): Unit = {
    
    val b = new TicTacToeBoard()
    
    println("Hello, world!")
    
    val x_next = true;
    
    b.printBoard();
    while (b.getWinner()=="_") {
      print("X to move:");
      val inp = scala.io.StdIn.readLine();
      b.printBoard();
    }
    println("Winner: " + b.getWinner());
    
  } 
}